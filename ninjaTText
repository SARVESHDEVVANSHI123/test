#include <stdio.h>

int main(void) {
    int len;
    int arr[len];
    int k,i,j, count=0;
    scanf("%d",&len);
    for(i=0;i<len;i++){
        scanf("%d",&arr[i]);
    }
    scanf("%d",&k);
    for(i=0;i<len;i++){
        for(j=i+1;j<len;j++){
            if(abs(arr[i]-arr[j]) == k)
               { count++;
                   //printf("(%d,%d)\n",arr[i],arr[j]);
               }
        }
    }
    printf("%d",count);
	return 0;
}
